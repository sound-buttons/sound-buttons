{
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "cSpell.words": [
    "Animista",
    "azureedge",
    "chara",
    "cssanimista",
    "devkit",
    "enablejsapi",
    "googletagmanager",
    "gtag",
    "heatmaps",
    "lightgreen",
    "modestbranding",
    "nocookie",
    "noopener",
    "playsinline",
    "rightclick",
    "soundbutton",
    "soundbuttons",
    "staticwebapp",
    "tocbot",
    "typeahead",
    "vendure",
    "webp",
    "widgetid",
    "youtu"
  ]
}
