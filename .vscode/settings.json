{
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "cSpell.words": [
    "azureedge",
    "chara",
    "devkit",
    "enablejsapi",
    "googletagmanager",
    "gtag",
    "heatmaps",
    "nocookie",
    "noopener",
    "playsinline",
    "rightclick",
    "soundbuttons",
    "typeahead",
    "vendure",
    "widgetid",
    "youtu"
  ]
}
